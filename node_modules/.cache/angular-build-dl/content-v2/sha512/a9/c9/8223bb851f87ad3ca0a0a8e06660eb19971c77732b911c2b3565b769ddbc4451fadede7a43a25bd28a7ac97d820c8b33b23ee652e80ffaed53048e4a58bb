etPatientChat(this.userdetail.userID,\"2020BM15\");\r\n            this.authService\r\n            .getPtCircleList(this.userdetail.userID)\r\n            .subscribe(data=>{\r\n              console.log(\"Circle data\", data);\r\n              this.circleDetail = data;\r\n              if(data!=null)\r\n              {\r\n                this.downloadDocument(this.circleDetail[0].vhEntity.circlePicture);\r\n              }              \r\n            });\r\n          }\r\n        });\r\n      }\r\n    }); \r\n       \r\n   }\r\n\r\n  ngOnInit() {\r\n  }\r\n\r\n  downloadDocument(keyname: any) {    \r\n    this.authService.downloadDocumentByKeyname(keyname)\r\n    .subscribe(data=>{\r\n    // debugger;\r\n    // console.log(data);\r\n    // console.log(\"network image data---\",data);  \r\n    this.circlePicture = 'data:image/jpeg;base64,'+data;\r\n    this.networkPictureStatus=true;\r\n    });\r\n    return this.circlePicture;\r\n  }\r\n\r\n  getPatientChat(userid,circleCode) {\r\n    this.authService\r\n    .getPatientChatByPatientUserid(userid,circleCode)\r\n    .subscribe(data=>{\r\n      this.ptChatDetail=data;\r\n    })\r\n  }\r\n\r\n  chatDoctor(docdetail:any, circledetail:any) {\r\n    for(let tc of circledetail.ptRefDocChat) {\r\n      if(docdetail.docProfile.docUserID==tc.triageDocUserID) {\r\n        console.log(\"doc match\", tc);\r\n        let navigationExtras: NavigationExtras = {\r\n          state: {\r\n            roomKey: tc.chatRoomKey,\r\n            roomName: tc.triageDocName,\r\n            name: tc.ptName\r\n          }\r\n        };\r\n        this.router.navigate(['chatroom'],navigationExtras);\r\n      }\r\n    }\r\n    \r\n  }\r\n\r\n  openChat(circle:any) {\r\n    this.authService\r\n    .getPatientChatByPatientUserid(this.userdetail.userID,circle.vhEntity.circleCode)\r\n    .subscribe(data=>{\r\n      this.ptChatDetail=data;\r\n      let navigationExtras: NavigationExtras = {\r\n        state: {\r\n          roomKey: this.ptChatDetail.chatRoomKey,\r\n          roomName:this.ptChatDetail.doctorName,\r\n          name:this.ptChatDetail.patientName\r\n        }\r\n      };\r\n      this.router.navigate(['chatroom'],navigationExtras);\r\n    });\r\n  }\r\n\r\n  ptlistFn(event) {\r\n    this.cdStatus = event;\r\n  }\r\n}\r\n"]}