onPage,{dxID:dxid});\r\n          }    \r\n        }\r\n        else {\r\n          let doc=this.doctors.find(item => item.docUserID === docuid);\r\n          console.log(JSON.stringify(doc));\r\n          console.log(\"dxid===\"+dxid);\r\n          console.log(\"docuid===\"+docuid);\r\n          this.authService\r\n          .uniquePrescription(dxid) \r\n          .subscribe(data=>{\r\n            console.log(data);\r\n            this.uniquePresc=data;\r\n            this.viewPrescription(doc, this.uniquePresc);\r\n          });\r\n        }\r\n      });\r\n    } else if(prescriptionType==\"Patient\") {\r\n      this.viewDocuments(dxid);\r\n    }\r\n  }\r\n\r\n  viewPrescription(doc:any,uniquePresc:any){\r\n    let navigationExtras: NavigationExtras = {\r\n      state: {\r\n        docdetail: doc,\r\n        userdetail: this.userdetail,\r\n        dxid: uniquePresc\r\n      }\r\n    };\r\n    console.log(\"navig extras\",navigationExtras);\r\n    this.router.navigate(['pt-eprescription'], navigationExtras);\r\n  }\r\n\r\n  PrescriptionSearch(value:any){\r\n    console.log(\"Form Value\",value);\r\n    // var dxfd = this.diagsearch.getValue();\r\n    var docuid = value.searchDoctor;\r\n    // var hospname = this.hospsearch.getValue();\r\n    var date = value.searchDate;\r\n    let search={\r\n      dxfd:\"\",\r\n      docuid:\"\",\r\n      hospname:\"\",\r\n      date:\"\",\r\n      userid:this.userID\r\n    }\r\n    if(date!=undefined) {\r\n      search.date = date;\r\n    }\r\n    // if(dxfd!=null && dxfd!=undefined) {\r\n    //   search.dxfd = dxfd;\r\n    // }\r\n    // if(docuid!=null && docuid!=undefined) {\r\n    //   if(this.docStatus==1){\r\n    //     search.docuid = docuid.docUserID;\r\n    //   }\r\n    // }\r\n    // if(hospname!=null && hospname!=undefined) {\r\n    //   search.hospname = hospname;\r\n    // }\r\n    if(search.docuid==\"\") {\r\n      this.docStatus=0;\r\n    }\r\n    console.log(\"prescription search 1\", search);\r\n    this.pagedItems=[];\r\n    if(this.docStatus==2) {\r\n      console.log(\"if of searchAPI\");\r\n      this.authService\r\n      .docUIDSearch(this.docUid)\r\n      .subscribe(data=>{\r\n        console.log(\"oninput docuid\", data);\r\n        if(data!=null) {\r\n          let docObj:any = data;\r\n          search.docuid = docObj.docUserID;    \r\n          this.authService\r\n          .prescriptionSearch(search)\r\n          .subscribe(data=>{\r\n            console.log(\"prescription list\", data);\r\n            if(data!=null) {\r\n              this.allItems = data;\r\n              if(this.allItems.length>0) {\r\n                this.msgblk=false;\r\n                this.setPage(1);    \r\n              } else {\r\n                this.msgblk=true;\r\n                this.setPage(0);\r\n              }\r\n            } else {\r\n              this.msgblk=true;\r\n            }\r\n          });\r\n        } else {\r\n          this.msgblk=true;\r\n        }\r\n      });\r\n    }\r\n    else {\r\n      console.log(\"else of searchAPI\");\r\n      this.authService\r\n      .prescriptionSearch(search)\r\n      .subscribe(data=>{\r\n        console.log(\"prescription list\", data);\r\n        if(data!=null) {\r\n          this.allItems = data;\r\n          if(this.allItems.length>0) {\r\n            this.msgblk=false;\r\n            this.setPage(1);    \r\n          }\r\n          else {\r\n            this.msgblk=true;\r\n            this.setPage(0);\r\n          }\r\n        }\r\n        else {\r\n          this.msgblk=true;\r\n        }\r\n      });\r\n    }\r\n  }\r\n\r\n  getVal(ev) {\r\n    //this.docsearch.writeValue(ev.docUserID);\r\n    // console.log(\"doc get val\", ev);\r\n    this.docStatus = 1;\r\n  }\r\n\r\n  keyUp(event) {\r\n    console.log(\"keydown event\", event.key);\r\n    this.keyCode = event.keyCode;\r\n  }\r\n\r\n  onInput($event, field: String) {\r\n    this.autoValue = \"no\";\r\n    console.log(\"oninput\",$event);\r\n    switch(field){\r\n      c