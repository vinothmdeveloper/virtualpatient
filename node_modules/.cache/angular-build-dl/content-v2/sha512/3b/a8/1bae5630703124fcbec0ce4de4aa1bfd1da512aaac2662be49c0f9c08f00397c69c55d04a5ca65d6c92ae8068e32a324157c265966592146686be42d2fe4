is.content.scrollToBottom(300);\r\n          }\r\n        }, 1000);\r\n      });\r\n  \r\n      // this.autoRefreshChat.on('value', resp => {\r\n      //   console.log(\"auto triage values---->\", resp.val());\r\n      //   let res = resp.val();\r\n      //   if(res.trigger) {\r\n      //     this.autoRefreshApicall();\r\n      //     this.autoRefreshChat.child(\"trigger\").update(false);\r\n      //   }      \r\n      // });\r\n    }    \r\n  }\r\n\r\n  autoRefreshApicall() {\r\n    if(this.role==\"Admin\") {\r\n      this.authService\r\n      .getPatientChatByPatientUserid(this.ptDetail.userID, this.circleCode)\r\n      .subscribe(data=>{\r\n        this.ptChatDetail=data;\r\n        console.log(\"ptchat detail\", this.ptChatDetail);\r\n        this.status = this.ptChatDetail.status;\r\n      });  \r\n    } else {\r\n      this.authService\r\n      .getPtTriageDocChatDetail(this.ptDetail.userID, this.docDetail.docUserID, this.circleCode)\r\n      .subscribe(data=>{\r\n        this.ptChatDetail=data;\r\n        console.log(\"ptchat detail\", this.ptChatDetail);\r\n        this.status = this.ptChatDetail.status;\r\n      });\r\n    }\r\n    \r\n    if(this.ptChatDetail != undefined) {\r\n      \r\n      let chatObj:any = {\r\n        userID: this.ptDetail.userID,\r\n        circleCode: this.circleCode\r\n      }\r\n      this.authService\r\n      .getChatRestrictDetail(chatObj)\r\n      .subscribe(data=>{\r\n        console.log(\"data\", data);\r\n        let chatDetail:any=data;\r\n        this.chatRestrictDetail=data;\r\n        this.chatCount = 5-chatDetail.chatCount;\r\n        let billObj:any = {\r\n          patientUserID: this.ptDetail.userID,\r\n          docUserID: this.docDetail.docUserID,\r\n          circleCode: this.circleCode\r\n        }\r\n        this.authService\r\n        .getConsultFeeDetail(billObj)\r\n        .subscribe(data=>{\r\n          console.log(\"data\", data);        \r\n          if (data!=null) {\r\n            this.feeDetail=data;\r\n            this.paymentStatus=this.feeDetail.paymentStatus;\r\n            if (this.feeDetail.paymentStatus==\"Pending\") {\r\n            console.log(\"pending payment\");\r\n            this.consultPayment();\r\n            }\r\n            //  else if( ){\r\n            //   console.log(\"paid payment\");\r\n            //   this.complaintCheck();\r\n            // }\r\n          } else {\r\n            console.log(\"no payment\", this.status, this.chatRestrictDetail);\r\n            if (this.chatCount>=1) {\r\n              if(this.role==\"Admin\" && this.status==\"Deactive\" && this.chatRestrictDetail.status==\"Deactive\") {\r\n                this.getComplaints();\r\n              } else if(this.role==\"Admin\" && this.status==\"Deactive\" && this.chatRestrictDetail.status==\"Active\") {\r\n                // this.complaintCheck();\r\n              } \r\n            } else {\r\n              alert(\"You have reached your maximum free chat for this month\");\r\n            }        \r\n          }\r\n        });      \r\n      });\r\n    }\r\n  }\r\n\r\n  // send complaint to triage doctor\r\n  async sendComplaintToTriageDoc() {\r\n    let adminDoc:any = this.circleDetail.cdProfileRole.filter(data =>\r\n      data.role == \"Admin\"\r\n    );\r\n    console.log(\"join admin\", adminDoc);\r\n    let chatformatobj:any={\r\n      userId: this.ptDetail.userID,\r\n      docUserId: adminDoc[0].docProfile.docUserID,\r\n      circleCode: this.circleCode\r\n    }\r\n    console.log(\"data\", chatformatobj);\r\n    this.authService\r\n    .getChatFormatDetailPt(chatformatobj) \r\n    .subscribe(async data=>{\r\n      let ptChatFormat=data;\r\n      console.log(\"chat format\", ptChatFormat);\r\n      if (ptChatFormat!=null) {\r\n        const modal = await this.modalCtrl.create({\r\n          component: SendTriageDocComplaintPage,\r\n          componentProps: {\r\n            circleName: this.circleDetail.vhEntity.circleName,\r\n            docDetail: this.docDetail,\r\n            docName:this.roomName,\r\n            circleCode: this.circleCode,\r\n            ptDetail: this.ptDetail,\r\n            ptChatFormatDetail: ptChatFormat[0],\r\n            triageChatDetail: this.triageChatDetail,\r\n            adminDocDetail: adminDoc\r\n          }\r\n        });\r\n        await modal.present();  \r\n      }\r\n    });\r\n  }\r\n\r\n\r\n  // get health complaints from patient modal box\r\n  async getComplaints() {\r\n    const modal = await this.modalCtrl.create({\r\n      component: SendChatComplaintsPage,\r\n      componentProps: {\r\n        circleName: this.circleDetail.vhEntity.circleName,\r\n        docDetail: this.docDetail,\r\n        docName:this.roomName,\r\n        circleCode: this.circleCode,\r\n        ptDetail: this.ptDetail,\r\n        chatCount: this.chatCount\r\n      }\r\n    });\r\n    await modal.present();\r\n    modal.onDidDismiss().then(data=>{\r\n      this.sendComplaint = data;\r\n      let newData = firebase.database().ref('chatrooms/'+this.roomkey+'/chats').push();\r\n      newData.set({\r\n        type: \"complaint\",\r\n        user:this.data.nickname,\r\n        message: this.complaintMessage(),\r\n        sendDate:Date(),\r\n        userID: this.loginUserDetail.userID,\r\n        viewStatus: 'No'\r\n      });\r\n      let chatcomplaintobj:any ={\r\n        subscriptionId: this.chatRestrictDetail.subscriptionId,\r\n        userId: this.ptDetail.createdByUserID,\r\n        docUserId: this.docDetail.docUserID,\r\n        circleCode: this.circleCode,\r\n        chatKey: this.roomkey,\r\n        createdDateTime: new Date(),\r\n        complaint: this.sendComplaint.data.complaint,\r\n        howLong: this.sendComplaint.data.howlong,\r\n        preExistingIllness: this.sendComplaint.data.preillness\r\n      };\r\n      console.log(\"chatresctrict\", chatcomplaintobj);\r\n      this.authService\r\n      .addChatComplaint(chatcomplaintobj)\r\n      .subscribe(data=>{\r\n        console.log(\"data\", data);\r\n        //let newData = this.autoRefreshChat.set({\r\n         // trigger: true\r\n        //});\r\n      });\r\n      \r\n      this.data.message = '';\r\n    });\r\n  }\r\n\r\n  complaintCheck() {\r\n    alert(\"Please wait till doctor accept your chat\");\r\n  }\r\n\r\n\r\n  // send message to doctor\r\n  sendMessage() {\r\n    let newData = firebase.database().ref('chatrooms/'+this.roomkey+'/chats').push();\r\n    newData.set({\r\n      type:this.data.type,\r\n      user:this.data.nickname,\r\n      message:this.data.message,\r\n      sendDate:Date(),\r\n      userID: this.loginUserDetail.userID,\r\n      viewStatus: 'No'\r\n    });\r\n    this.data.message = '';\r\n  }\r\n\r\n  // view prescription from chat\r\n  openEPrescription(chat:any) {\r\n    console.log(\"prescription\",this.docDetail);\r\n    this.authService\r\n    .uniquePrescription(chat.prescID) \r\n    .subscribe(data=>{\r\n      console.log(data);\r\n      this.uniquePresc=data;\r\n      let presc = {\r\n        docdetail: this.docDetail,\r\n        dxid: this.uniquePresc\r\n      }\r\n      this.dataService.setData(24, presc);\r\n      this.router.navigateByUrl('pt-eprescription/24');\r\n    });\r\n\r\n  }\r\n\r\n  //Add  health complaints to family doc chat\r\n  complaintMessage() {\r\n    let chatComplaint: string=\"\";\r\n    if (this.sendComplaint.data.howlong!=\"\" && this.sendComplaint.data.preillness!=\"\") {\r\n      chatComplaint=\"General Health Complaint :\"+\"\\n\"+this.sendComplaint.data.complaint+'\\n\\n'+\r\n                    \"How long have you been experiencing said complaint ?\"+'\\n'+this.sendComplaint.data.howlong+'\\n\\n'+\r\n                    \"Pre-existing illness,Current medication, Drug allergy :\"+'\\n'+this.sendComplaint.data.preillness;  \r\n    }else if(this.sendComplaint.data.howlong!=\"\" && this.sendComplaint.data.preillness==\"\") {\r\n      chatComplaint=\"General Health Complaint :\"+\"\\n\"+this.sendComplaint.data.complaint+'\\n\\n'+\r\n                    \"How long have you been experiencing said complaint ?\"+'\\n'+this.sendComplaint.data.howlong;\r\n    }else if(this.sendComplaint.data.howlong==\"\" && this.sendComplaint.data.preillness!=\"\") {\r\n      chatComplaint=\"General Health Complaint :\"+\"\\n\"+this.sendComplaint.data.complaint+'\\n\\n'+\r\n                    \"Pre-existing illness,Current medication, Drug allergy :\"+'\\n'+this.sendComplaint.data.preillness;  \r\n    }else if(this.sendComplaint.data.howlong==\"\" && this.sendComplaint.data.preillness==\"\") {\r\n      chatComplaint=\"General Health Complaint :\"+\"\\n\"+this.sendComplaint.data.complaint;\r\n    }\r\n    console.log(\"complaint\", chatComplaint);\r\n    return chatComplaint;\r\n  }\r\n\r\n  // for recording audio\r\n  startRecording() {\r\n    if (!this.isRecording) {\r\n      this.isRecording = true;\r\n      this.audioRecordingService.startRecording();\r\n    }\r\n  }\r\n\r\n  stopRecording() {\r\n    // add s3 link\r\n    if (this.isRecording) {\r\n      this.audioRecordingService.stopRecording();\r\n      this.isRecording = false;\r\n    }\r\n    this.presentToast('Verify to send');\r\n  }\r\n\r\n  abortRecording() {\r\n    if (this.isRecording) {\r\n      this.isRecording = false;\r\n      this.audioRecordingService.abortRecording();\r\n    }\r\n  }\r\n\r\n  ngOnDestroy(): void {\r\n    this.abortRecording();\r\n  }\r\n\r\n  clearRecordedData() {\r\n    this.blobUrl = null;\r\n  }\r\n\r\n  async storeAudio(){\r\n    // this.loading = this.loadingCtrl.create({\r\n    //   message: 'Uploading...',\r\n    // });\r\n    // (await this.loading).present();\r\n    const loading = await this.loadingCtrl.create({\r\n      message: 'Uploading...'\r\n      \r\n    });\r\n    await loading.present();\r\n    let body = new FormData();\r\n    this.keyname=\"ChatTestRecordFile\"+this.myRand+\".wav\";\r\n    // Add file content to prepare the request    \r\n    body.append('uploadfile', this.blobData);\r\n    body.append('keyname', this.keyname);\r\n    // Launch post request\r\n    this.http.post(UPLOAD_URL+'/api/file/lockdownupload', body)\r\n    .subscribe(\r\n      // Admire results\r\n      async (data) => {\r\n        console.log(data);\r\n        await loading.dismiss();\r\n        //this.blobdata=null; \r\n        this.clearRecordedData();\r\n      },\r\n      () => { \r\n        this.clearRecordedData()\r\n        let newData = firebase.database().ref('chatrooms/'+this.roomkey+'/chats').push();\r\n        newData.set({\r\n          type:\"audio\",\r\n          user:this.data.nickname,\r\n          message:\"https://swasthprescription.s3-ap-southeast-1.amazonaws.com/\"+this.keyname,\r\n          sendDate:Date(),\r\n          userID: this.loginUserDetail.userID,\r\n          viewStatus: 'No'\r\n        });\r\n        console.log(\"Ouput\",this.data.type,this.blobData);\r\n        this.data.message = '';\r\n        \r\n      this.presentToast('Voice note sent');\r\n        console.log(\"completed\");\r\n      }\r\n    );\r\n  }\r\n\r\n  public async presentActionSheet() {\r\n    let actionSheet = this.actionSheetCtrl.create({\r\n      buttons: [\r\n        {\r\n          text: 'Load from Library',\r\n          handler: () => {\r\n            this.takePicture(this.camera.PictureSourceType.PHOTOLIBRARY);\r\n          }\r\n        },\r\n        {\r\n          text: 'Use Camera',\r\n          handler: () => {\r\n            this.takePicture(this.camera.PictureSourceType.CAMERA);\r\n          }\r\n        }\r\n      ]\r\n    });\r\n    (await actionSheet).present();\r\n  }\r\n\r\n  public takePicture(sourceType) {\r\n    // Create options for the Camera Dialog\r\n    var options = {\r\n      quality: 100,\r\n      sourceType: sourceType,\r\n      saveToPhotoAlbum: false,\r\n      correctOrientation: true\r\n    };\r\n    // Get the data of an image\r\n    this.camera.getPicture(options).then((imagePath) => {\r\n      this.imgPath=imagePath;\r\n      // Special handling for Android library\r\n      if (this.platform.is('android') && sourceType === this.camera.PictureSourceType.PHOTOLIBRARY) {\r\n        this.filePath.resolveNativePath(imagePath)\r\n        .then(filePath => {\r\n          let correctPath = filePath.substr(0, filePath.lastIndexOf('/') + 1);\r\n          let currentName = imagePath.substring(imagePath.lastIndexOf('/') + 1, imagePath.lastIndexOf('?'));\r\n          this.copyFileToLocalDir(correctPath, currentName, this.createFileName());\r\n        });\r\n        this.uploadImage(this.imgPath);\r\n      } else {\r\n        var currentName = imagePath.substr(imagePath.lastIndexOf('/') + 1);\r\n        var correctPath = imagePath.substr(0, imagePath.lastIndexOf('/') + 1);\r\n        this.copyFileToLocalDir(correctPath, currentName, this.createFileName());\r\n        this.uploadImage(this.imgPath);\r\n      }\r\n    }, (err) => {\r\n      this.presentToast('Error while selecting image.');\r\n    });\r\n  }\r\n\r\n  public async uploadImage(img) {\r\n    // this.loading = this.loadingCtrl.create({\r\n    //   message: 'Uploading...',\r\n    // });\r\n    // this.loading.present();\r\n    const loading = await this.loadingCtrl.create({\r\n      message: 'Uploading...'\r\n    });\r\n    await loading.present();\r\n    this.keyname=\"ChatImage\"+this.myRand+'.jpg';\r\n    let body = new FormData();\r\n    // Add file content to prepare the request\r\n    body.append('uploadfile', img);\r\n    body.append('keyname', this.keyname);\r\n    // Launch post request\r\n    this.http.post(UPLOAD_URL+'/api/file/lockdownupload', body)\r\n    .subscribe(\r\n      // Admire results\r\n      async (data) => {console.log(data)\r\n        await loading.dismiss();\r\n        this.presentToast('Image succesful uploaded.');\r\n      },\r\n      // Or errors :-(\r\n      async error => {console.log(error)\r\n        await loading.dismiss();\r\n        this.presentToast('Error while uploading file.');\r\n      },\r\n      // tell us if it's finished\r\n      async () => { console.log(\"completed\") \r\n        await loading.dismiss();\r\n        this.presentToast('Image succesful uploaded.');\r\n      }\r\n    );\r\n    // Destination URL\r\n    // console.log(\"Upload strated\");\r\n    // var url = \"http://yoururl/upload.php\";\r\n    // // File for Upload\r\n    // var targetPath = this.pathForImage(this.lastImage);\r\n    // // File name only\r\n    // var filename = this.lastImage;\r\n    // var options = {\r\n    //   fileKey: \"file\",\r\n    //   fileName: filename,\r\n    //   chunkedMode: false,\r\n    //   mimeType: \"multipart/form-data\",\r\n    //   params : {'fileName': filename}\r\n    // };\r\n    // const fileTransfer: TransferObject = this.transfer.create();\r\n    // this.loading = this.loadingCtrl.create({\r\n    //   content: 'Uploading...',\r\n    // });\r\n    // this.loading.present(); \r\n    // // Use the FileTransfer to upload the image\r\n    // fileTransfer.upload(targetPath, url, options).then(data => {\r\n    //   this.loading.dismissAll()\r\n    //   this.presentToast('Image succesful uploaded.');\r\n    // }, err => {\r\n    //   this.loading.dismissAll()\r\n    //   this.presentToast('Error while uploading file.');\r\n    // });\r\n  }\r\n\r\n  // Copy the image to a local folder\r\n  private copyFileToLocalDir(namePath, currentName, newFileName) {\r\n    this.file.copyFile(namePath, currentName, cordova.file.dataDirectory, newFileName).then(success => {\r\n      this.lastImage = newFileName;\r\n    }, error => {\r\n      this.presentToast('Error while storing file.');\r\n    });\r\n  }\r\n\r\n  private createFileName() {\r\n    var d = new Date(),\r\n    n = d.getTime(),\r\n    newFileName =  n + \".jpg\";\r\n    return newFileName;\r\n  }\r\n\r\n  // exit chat room \r\n  exitChat() {\r\n    let exitData = firebase.database().ref('chatrooms/'+this.roomkey+'/chats').push();\r\n    exitData.set({\r\n      type:'exit',\r\n      user:this.nickname,\r\n      message:this.nickname+' has exited this room.',\r\n      sendDate:Date()\r\n    });\r\n    this.offStatus = true;\r\n  }\r\n\r\n  // alert for empty message send\r\n  empty(){\r\n    this.presentToast(\"Empty message\");\r\n  }\r\n\r\n  private async presentToast(text) {\r\n    let toast = this.toastCtrl.create({\r\n      message: text,\r\n      duration: 3000,\r\n      position: 'top'\r\n    });\r\n    (await toast).present();\r\n  }\r\n\r\n  async consultPayment() {\r\n    const alert = this.alertCtrl.create({\r\n      header: 'Payment',\r\n      message: 'Pay your consultation fee to the doctor for further accessing of all swasth features',\r\n      buttons: [\r\n        {\r\n          text: 'Pay',\r\n          handler: () => {\r\n            console.log('yes');\r\n            this.feePayment();\r\n          }\r\n        },\r\n        {\r\n          text: 'Cancel',\r\n          handler: () => {\r\n            console.log(\"cancel\");\r\n          }\r\n        }]\r\n      });\r\n      (await alert).present();\r\n  }\r\n\r\n  // payment for consultation fee\r\n  feePayment() {\r\n    var options = {\r\n      description: 'Credits towards consultation',\r\n      // image: 'https://i.imgur.com/3g7nmJC.png',\r\n      currency: 'INR',\r\n      key: 'rzp_test_SeRkqskw09t911',\r\n      amount: this.feeDetail.transAmount * 100,\r\n      name: this.ptDetail.ptFirstName+\" \"+this.ptDetail.ptLastName,\r\n      'handler': this.paymentCapture.bind(this),\r\n      prefill: {\r\n        email: this.ptDetail.ptEmailID,\r\n        contact: this.ptDetail.ptMobileNo,\r\n        name: this.ptDetail.ptFirstName+\" \"+this.ptDetail.ptLastName,\r\n        patient_uid:this.ptDetail.userID,\r\n        doctor_uid: this.docDetail.docUserID\r\n      },\r\n      theme: {\r\n        color: '#F37254'\r\n      },\r\n      modal: {\r\n        ondismiss: () => { // <- Here!\r\n          //alert('dismissed')\r\n        }\r\n      }\r\n    }\r\n    var successCallback = (payment_id) => { // <- Here!\r\n      alert('payment_id: ' + payment_id);\r\n      // this.navCtrl.push(\"ThankyouPage\",{\r\n      //         status: this.status\r\n      //     });\r\n    };\r\n    var cancelCallback = (error) => { // <- Here!\r\n      alert(error.description + ' (Error ' + error.code + ')');\r\n    };\r\n    Razorpay.open(options, successCallback, cancelCallback);\r\n  }\r\n  \r\n  paymentCapture(response) { \r\n    this.paymentId = response.razorpay_payment_id;\r\n    //alert(this.paymentId);\r\n    console.log(\"payment id \"+this.paymentId, this.feeDetail);\r\n    if(this.paymentId){\r\n      this.paymentTransaction={\r\n        transactionID: this.feeDetail.transactionID,\r\n        paymentID: this.paymentId,\r\n        paymentStatus:\"Paid\",\r\n        transactionDateTime:new Date(),\r\n        transAmount: this.feeDetail.transAmount,\r\n        circleCode: this.feeDetail.circleCode,\r\n        circleAdminUserID: this.feeDetail.circleAdminUserID,\r\n        docUserID: this.feeDetail.docUserID,\r\n        patientUserID: this.feeDetail.patientUserID,\r\n        createdByUserID: this.feeDetail.createdByUserID,\r\n        createdDateTime: this.feeDetail.createdDateTime,\r\n        ptDxId: this.feeDetail.ptDxId\r\n      }\r\n      console.log(\"if condition\", this.paymentTransaction);\r\n      this.authService\r\n      .updateConsultFee(this.paymentTransaction)\r\n      .subscribe(data=>{\r\n        console.log(\"Payment Details\", data);\r\n        this.paymentDetail = data;\r\n        alert(\"payment success\");\r\n      });    \r\n    }else {\r\n      this.paymentTransaction={\r\n        transactionID: this.feeDetail.transactionId,\r\n        paymentID: this.paymentId,\r\n        paymentStatus:\"Pending\",\r\n        transactionDateTime:new Date(),\r\n        transAmount: this.feeDetail.transAmount,\r\n        circleCode: this.feeDetail.circleCode,\r\n        circleAdminUserID: this.feeDetail.circleAdminUserID,\r\n        docUserID: this.feeDetail.docUserID,\r\n        patientUserID: this.feeDetail.patientUserID,\r\