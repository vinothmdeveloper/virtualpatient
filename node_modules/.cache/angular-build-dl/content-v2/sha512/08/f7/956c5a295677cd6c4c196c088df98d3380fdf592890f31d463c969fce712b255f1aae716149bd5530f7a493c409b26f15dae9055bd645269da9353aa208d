) {\r\n    if (this.activetedRoute.snapshot.data['ctData']) {\r\n      let data = this.activetedRoute.snapshot.data['ctData'];\r\n      this.roomkey = data.roomKey;\r\n      this.nickname = data.name;\r\n      this.roomName = data.roomName;\r\n    }\r\n    console.log(\"Room Name=>\",this.roomName);\r\n    this.data.type = 'message';\r\n    this.data.nickname = this.nickname;\r\n    this.data.message = '';\r\n\r\n    this.getComplaints();\r\n    \r\n    firebase.database().ref('chatrooms/'+this.roomkey+'/chats').on('value', resp => {\r\n      this.chats = [];\r\n      this.chats = snapshotToArray(resp);\r\n      console.log(\"chats\", this.chats);\r\n      setTimeout(() => {\r\n        if(this.offStatus === false) {\r\n          this.content.scrollToBottom(300);\r\n        }\r\n      }, 1000);\r\n    });\r\n  }\r\n\r\n  // get health complaints from patient modal box\r\n  async getComplaints() {\r\n    const modal = await this.modalCtrl.create({\r\n      component: SendChatComplaintsPage,\r\n      componentProps: {\r\n        circleName: this.circleName,\r\n        docSpec: this.docSpec,\r\n        docName:this.roomName\r\n      }\r\n    });\r\n    await modal.present();\r\n    modal.onDidDismiss().then(data=>{\r\n      this.sendComplaintStatus=true;\r\n      this.sendComplaint = data;\r\n      let newData = firebase.database().ref('chatrooms/'+this.roomkey+'/chats').push();\r\n      newData.set({\r\n        type: \"complaint\",\r\n        user:this.data.nickname,\r\n        message: this.complaintMessage(),\r\n        sendDate:Date()\r\n      });\r\n      this.data.message = '';\r\n    });\r\n  }\r\n\r\n  // send message to doctor\r\n  sendMessage() {\r\n    let newData = firebase.database().ref('chatrooms/'+this.roomkey+'/chats').push();\r\n    newData.set({\r\n      type:this.data.type,\r\n      user:this.data.nickname,\r\n      message:this.data.message,\r\n      sendDate:Date()\r\n    });\r\n    this.data.message = '';\r\n  }\r\n\r\n  //Add  health complaints t