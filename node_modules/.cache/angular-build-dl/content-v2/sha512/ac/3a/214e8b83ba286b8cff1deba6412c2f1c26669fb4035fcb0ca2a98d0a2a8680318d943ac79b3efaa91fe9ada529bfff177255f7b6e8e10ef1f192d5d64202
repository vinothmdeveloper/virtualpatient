atus = tc.status; \r\n        this.docDetail=docdetail.docProfile;\r\n        chatRoomKey = tc.chatRoomKey;\r\n        ptName = tc.ptName;\r\n        triageDocName = tc.triageDocName;\r\n        videoCallLink=tc.videoCallLink;\r\n      \r\n        /* let navigationExtras: NavigationExtras = {\r\n          state: {\r\n            roomKey: tc.chatRoomKey,\r\n            roomName: tc.triageDocName,\r\n            name: tc.ptName,\r\n          }\r\n        };\r\n        this.router.navigate(['chatroom'],navigationExtras); */\r\n      }\r\n    }\r\n    this.chatRoomFn(chatRoomKey, triageDocName, ptName, videoCallLink);  \r\n  }\r\n\r\n  openChat(circle:any) {\r\n    this.circleDetail=circle;\r\n    for (let cl of circle.cdProfileRole) {\r\n      if(cl.role==\"Admin\") {\r\n        this.docDetail=cl.docProfile;\r\n        this.role = cl.role;\r\n        \r\n      }\r\n    }\r\n    this.authService\r\n    .getPatientChatByPatientUserid(this.userdetail.userID,circle.vhEntity.circleCode)\r\n    .subscribe(data=>{\r\n      this.ptChatDetail=data;\r\n      console.log(\"ptchat detail\", this.ptChatDetail);\r\n      this.status = this.ptChatDetail.status;\r\n      this.chatRoomFn(this.ptChatDetail.chatRoomKey, this.ptChatDetail.doctorName, this.ptChatDetail.patientName,this.ptChatDetail.videoCallLink);\r\n      /* let navigationExtras: NavigationExtras = {\r\n        state: {\r\n          roomKey: this.ptChatDetail.chatRoomKey,\r\n          roomName:this.ptChatDetail.doctorName,\r\n          name:this.ptChatDetail.patientName\r\n        }\r\n      };\r\n      this.router.navigate(['chatroom'],navigationExtras); */\r\n    });\r\n  }\r\n\r\n  chatRoomFn(chRoomKey, docName, ptName,videocalllink) {\r\n    let ctRoom = {\r\n      roomKey: chRoomKey,\r\n      roomName: docName,\r\n      name: ptName,\r\n      circleCode: this.circleDetail.vhEntity.circleCode,\r\n      ptDetail: this.userdetail,\r\n      docDetail: this.docDetail,\r\n      role: this.role,\r\n      status: this.status,\r\n      circleDetail: this.circleDetail,\r\n      videocalllink:videocalllink\r\n    }\r\n    this.dataService.setData(29, ctRoom);\r\n    this.router.navigateByUrl('chatroom/29');\r\n  }\r\n\r\n  ptlistFn(event) {\r\n    this.cdStatus = event;\r\n  }\r\n\r\n  async sharePopover(ev: Event, cobj) {\r\n    // this.router.navigateByUrl('/circle');\r\n    console.log(\"share doc\", this.userdetail);\r\n    console.log(\"share circleobj\", cobj);\r\n    const popover = await this.popoverCtrl.create({      \r\n      component: ShareCirclePage,      \r\n      componentProps: {\r\n        circleObj: cobj,\r\n        ptDetail: this.userdetail\r\n      },\r\n      backdropDismiss: false,\r\n      animated: true,\r\n      // cssClass: 'mblk',\r\n      // event: ev\r\n    });\r\n    popover.onDidDismiss().then(data => {\r\n      console.log(\"data--\",data);\r\n      if(data == \"Cancel\") {\r\n        console.log(\"popover cancelled\");\r\n      } else {\r\n        let res:any=data;\r\n        console.log(\"popover dismiss else\",res);\r\n        if(res.data.result == \"Success\") {\r\n          console.log(\"popover dismiss else circle code \",res.data.circleCode);          \r\n        }\r\n      }      \r\n    });    \r\n    popover.present();\r\n    // this.router.navigate(['circle']);\r\n  }\r\n\r\n  logout() {\r\n    this.loginAuth.logout();\r\n  }\r\n  \r\n}\r\n"]}